<CanvasExample>:
    canvas:
        Color:
            rgb: 1, 0.3, 0
        Line:
            circle: (200, 200, 100) # center_x, center_y, radius
            width: 2
        Color:
            rgb: 0.3, 1, 0
        Line:
            ellipse: (400, 400, 100, 200) # center_x, center_y, radius_x, radius_y
            width: 4
        Color:
            rgba: 0, 0.3, 1, 0.5 # 50% transparency
        Line:
            rectangle: (600, 100, 300, 200) # x, y, w, h
            width: 10

